{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.ignoreLimitWarning": true,
  "editor.formatOnSave": false,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true
  },
  "files.exclude": {
    // "**/package.json": true,
    // "**/package-lock.json": true,
    // "**/tsconfig.temp.json": true,
    // "**/tsconfig.json": true,

    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
//   "cSpell.words": ["gametest", "gametests", "mcaddon"],
  "editor.tabSize": 4,
  "eslint.useFlatConfig": true
}
